{
  "name": "audio-recording-manager",
  "version": "1.0.0",
  "description": "Audio Recording Manager - JavaScript test dependencies",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/tests/**/*.js"
    ],
    "collectCoverageFrom": [
      "static/js/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "verbose": true
  }
}
